{
  "name": "@openfn/language-http",
  "version": "4.0.0",
  "description": "An HTTP request language package for use with Open Function",
  "homepage": "https://docs.openfn.org",
  "repository": {
    "type": "git",
    "url": "https://github.com/openfn/language-http.git"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/language-http.d.ts",
  "scripts": {
    "build": "npm run clean && node build.mjs",
    "clean": "rimraf dist/ types/",
    "test": "mocha --require @babel/register",
    "test:watch": "mocha -w --require @babel/register",
    "ast": "simple-ast --adaptor ./src/Adaptor.js --output ast.json",
    "postversion": "git push && git push --tags",
    "version": "npm run build && git add -A lib ast.json"
  },
  "author": "Open Function Group",
  "license": "LGPLv3",
  "files": [
    "dist",
    "ast.json"
  ],
  "devDependencies": {
    "@babel/cli": "^7.16.6",
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@babel/register": "^7.9.0",
    "@microsoft/api-extractor": "^7.28.3",
    "@openfn/simple-ast": "^0.4.1",
    "@types/node": "^18.0.0",
    "chai": "^4.2.0",
    "cheerio": "^1.0.0-rc.10",
    "cheerio-tableparser": "^1.0.1",
    "csv-parse": "^4.10.1",
    "deep-eql": "^4.0.0",
    "dts-bundle-generator": "^6.12.0",
    "esbuild": "^0.14.48",
    "execa": "^6.1.0",
    "fast-safe-stringify": "^2.0.7",
    "form-data": "^3.0.0",
    "jsdoc": "^3.6.10",
    "lodash": "^4.17.19",
    "mocha": "^9.2.2",
    "nock": "^13.0.5",
    "request": "^2.88.2",
    "rimraf": "^3.0.2",
    "sinon": "^9.2.3",
    "tough-cookie": "^4.0.0",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "@openfn/language-common": "file:../language-common2/openfn-language-common-2.0.0-pre1.tgz"
  },
  "bundleDependencies": []
}
